{
  "name": "geop",
  "version": "1.6.1",
  "description": "Estonian geocaching game",
  "main": "index.js",
  "scripts": {
    "build": "webpack --mode development --devtool source-map",
    "build:test": "webpack --mode production --devtool source-map",
    "build:prod": "webpack --mode production",
    "build:translations": "python3 scripts/translations.py",
    "start": "webpack-dev-server --mode development --inline --hot --open",
    "start:ssl": "webpack-dev-server --mode development --port 3001 --inline --hot --https --open",
    "start:prod": "python3 -m http.server 3000 --directory dist/",
    "deploy": "npm run build:prod && ./scripts/deploy.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sookoll/geop.git"
  },
  "author": "Mihkel Oviir",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sookoll/geop/issues"
  },
  "homepage": "https://gp.sookoll.ee",
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^5.1.2",
    "css-loader": "^1.0.1",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.1.0",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.5",
    "style-loader": "^0.23.1",
    "stylus": "^0.54.8",
    "stylus-loader": "^3.0.2",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.2",
    "webpack-pwa-manifest": "^4.3.0",
    "workbox-webpack-plugin": "^3.6.3"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.2",
    "@mapbox/polyline": "^1.1.1",
